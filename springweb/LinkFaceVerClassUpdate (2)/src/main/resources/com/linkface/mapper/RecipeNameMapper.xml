<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.linkface.mapper.RecipeNameMapper">
	<select id="getRecipeName" resultType="com.linkface.entity.RecipeName">
		select * from foodname where recipe_nm_ko = ${recipenmko}
	</select>
	<select id="getRecipeList" resultType="com.linkface.entity.RecipeName">
		select * from foodname
	</select>

</mapper>